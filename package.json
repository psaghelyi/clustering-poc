{
  "name": "clustering-poc",
  "version": "1.0.0",
  "description": "Document clustering with S3-stored embeddings using AWS Bedrock (Nova, Titan, Cohere)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "merge": "tsx src/merge-documents.ts",
    "merge:aws": "aws-vault exec ${AWS_ACCOUNT:-acl-playground} -- npm run merge",
    "reindex": "tsx src/merge-documents.ts --reindex",
    "reindex:aws": "aws-vault exec ${AWS_ACCOUNT:-acl-playground} -- npm run reindex",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "clustering",
    "embeddings",
    "s3",
    "aws-bedrock",
    "cosine-similarity"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.709.0",
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/client-s3vectors": "^3.709.0",
    "density-clustering": "^1.3.0",
    "dotenv": "^17.2.3",
    "fast-cosine-similarity": "^1.0.0",
    "hdbscan-ts": "^1.0.16"
  },
  "devDependencies": {
    "@types/density-clustering": "^1.3.3",
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
