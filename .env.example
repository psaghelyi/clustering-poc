# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# S3 Configuration
S3_BUCKET=clustering-poc-embeddings

# Embedding Model (nova, titan, or cohere)
EMBEDDING_PROVIDER=nova

# Testing Options
USE_MOCK_EMBEDDINGS=true  # Set to false to use real AWS Bedrock embeddings
USE_LOCALSTACK=true       # Set to false to use real AWS S3
